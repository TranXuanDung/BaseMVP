<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <attr name="fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="StatusAvatarCircleImageView">
        <attr name="statusAngle" format="dimension"></attr>
        <attr name="sizeStatusCicle" format="dimension"></attr>
        <attr name="colorStatusCircle" format="color"></attr>
        <attr name="hasCircleStatus" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="SquareView">
        <attr name="is_square_width" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SquareViews"></declare-styleable>

</resources>